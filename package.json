{
  "name": "jajiautos-backend",
  "version": "1.0.0",
  "description": "Jaji Autos - Car Dealership Management System API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "npm run db:generate && tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:setup": "prisma migrate deploy && prisma generate",
    "render:build": "npm install && npx prisma db push --schema=src/prisma/schema.prisma && npx prisma generate --schema=src/prisma/schema.prisma && npm run build",
    "render:start": "npm start",
    "docker:build": "docker build -t jajiautos-backend .",
    "docker:run": "docker run -p 3000:3000 jajiautos-backend",
    "lint": "echo 'Linting not configured'",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "car-dealership",
    "inventory-management",
    "sales-tracking",
    "nodejs",
    "prisma",
    "postgresql"
  ],
  "author": "Jaji Autos Team",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.1",
    "@types/streamifier": "^0.1.2",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "sharp": "^0.34.2",
    "streamifier": "^0.1.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}